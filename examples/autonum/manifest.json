{
    "manifest_version": 1,
    "version": 2,
    "type": "APP",
    "name": {
        "ja": "自動採番プラグイン",
        "en": "Auto-numbering plug-in",
        "zh": "自动编号插件"
    },
    "description": {
        "ja": "保存時に自動採番するプラグインです。",
        "en": "Auto-numbering plug-in",
        "zh": "此插件可在保存记录时自动生成编号。"
    },
    "icon": "img/icon.png",
    "desktop": {
        "js": [
            "https://js.cybozu.com/jquery/2.1.3/jquery.min.js",
            "https://js.kintone.com/momentjs/2.14.1/moment-with-locales.min.js",
            "js/desktop.js"
        ],
        "css": [
            "css/51-modern-default.css",
            "css/desktop.css"
        ]
    },
    "config": {
        "html": "html/config.html",
        "js": [
            "https://js.cybozu.com/jquery/2.1.3/jquery.min.js",
            "https://js.kintone.com/momentjs/2.14.1/moment-with-locales.min.js",
            "https://js.kintone.com/jsrender/0.9.80/jsrender.min.js",
            "js/config.js"
        ],
        "css": [
            "https://js.kintone.com/sweetalert/v1.1.3/sweetalert.css",
            "css/51-modern-default.css",
            "css/config.css"
        ],
        "required_params": ["autoNumberingFieldcode", "format", "connective"]
    }
}
