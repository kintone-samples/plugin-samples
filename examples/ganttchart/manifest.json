         "https://js.cybozu.com/jquery/2.1.3/jquery.min.js"
      ]
   },
   "config":{
      "html":"html/config.html",
      "js":[
         "https://js.cybozu.com/jquery/2.1.3/jquery.min.js",
         "https://js.kintone.com/jsrender/0.9.80/jsrender.min.js",
         "thirdparties/jqColorPicker.min.js",
         "js/config.js"
      ],
      "css":[
         "css/51-current-default.css",
         "css/config.css"
      ],
      "required_params":[
         "ganttchartTitle",
         "ganttchartFrom",
